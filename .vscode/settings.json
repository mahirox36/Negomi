{
  "cSpell.words": [
    "autorole",
    "blurple",
    "colours",
    "cooldowns",
    "customemoji",
    "levelname",
    "Nexon",
    "putalpha",
    "roleplay",
    "Rolez",
    "slowmode",
    "Unmute",
    "unmuted",
    "Untimeout"
  ],
  "todo-tree.tree.showCountsInTree": true,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "log.error",
        "settings": {
          "foreground": "#af1f1f",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "log.info",
        "settings": {
          "foreground": "#1E90FF",
          "fontStyle": ""
        }
      },
      {
        "scope": "log.debug",
        "settings": {
          "foreground": "#6A5ACD",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "log.warning",
        "settings": {
          "foreground": "#f4ad42",
          "fontStyle": "bold"
        }
      }
    ]
  },
  "material-icon-theme.folders.associations": {
			"Features": "plugin",
			"OllamaSetup": "download",
			"Dashboard": "serverless",
			"Working on Progress": "Ci",
			"Nexon": "Quasar",
    		"Ollama": "Custom",
			"Roadmap": "Husky",
			"callback": "Pipe",
			"statistics": "Docs",
		},
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.sync": true,
    "**/builds": true,
    "**/logs": true
  },
  "python.analysis.typeCheckingMode": "basic"
}
